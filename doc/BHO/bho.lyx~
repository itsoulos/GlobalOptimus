\end_layout

\begin_layout Plain Layout

\size tiny
\begin_inset Formula $E(\mathbf{x}_{i})=\frac{1}{2}\sum_{j\neq i}f_{c}(r_{ij})\left[V_{R}(r_{ij})-B_{ij}V_{A}(r_{ij})\right]$
\end_inset


\end_layout

\begin_layout Plain Layout

\size tiny
where 
\begin_inset Formula $r_{ij}=\|\mathbf{x}_{i}-\mathbf{x}_{j}\|$
\end_inset

,
 
\begin_inset Formula $V_{R}(r)=A\exp(-\lambda_{1}r)$
\end_inset


\end_layout

\begin_layout Plain Layout

\size tiny
\begin_inset Formula $V_{A}(r)=B\exp(-\lambda_{2}r)$
\end_inset


\end_layout

\begin_layout Plain Layout

\size tiny
\begin_inset Formula $f_{c}(r)$
\end_inset

:
 cutoff function with 
\begin_inset Formula $f_{c}(r)$
\end_inset

:
 angle parameter
\end_layout

