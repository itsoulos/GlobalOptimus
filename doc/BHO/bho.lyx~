\size tiny
\begin_inset Formula $\min_{\mathbf{x}}\;V(\mathbf{x})=\sum_{i=1}^{N}\sum_{j>i}^{N}f_{C}(r_{ij})\left[a_{ij}f_{R}(r_{ij})+b_{ij}f_{A}(r_{ij})\right]$
\end_inset


\end_layout

\begin_layout Plain Layout

\size tiny
\begin_inset Formula $f_{C}(r)=\begin{cases}
1, & r<R-D\\
\frac{1}{2}+\frac{1}{2}\cos\left(\frac{\pi(r-R+D)}{2D}\right), & |r-R|\leq D\\
0, & r>R+D
\end{cases}$
\end_inset


\end_layout

\begin_layout Plain Layout

\size tiny
\begin_inset Formula $f_{R}(r)=A\exp(-\lambda_{1}r)$
\end_inset


\end_layout

\begin_layout Plain Layout

\size tiny
\begin_inset Formula $f_{A}(r)=-B\exp(-\lambda_{2}r)$
\end_inset


\end_layout

\begin_layout Plain Layout

\size tiny
\begin_inset Formula $b_{ij}=\left[1+(\beta^{n})\zeta_{ij}^{n}\right]^{-1/(2n)}$
\end_inset


\end_layout

\begin_layout Plain Layout

\size tiny
\begin_inset Formula $\sum_{k\neq i,j}f_{C}(r_{ik})g(\theta_{ijk})\exp\left[\lambda_{3}^{3}(r_{ij}-r_{ik})^{3}\right]$
\end_inset


\end_layout

\end_inset
</cell>
